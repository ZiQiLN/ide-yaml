{
  "name": "ide-yaml",
  "version": "0.1.6",
  "description": "Atom-IDE support for YAML language",
  "main": "src/main.js",
  "scripts": {
    "install": "napa",
    "postinstall": "./build-yaml-lang-server.sh",
    "lint": "eslint ./src",
    "test": "npm run lint"
  },
  "repository": "https://github.com/liuderchi/ide-yaml.git",
  "keywords": [
    "yaml",
    "yaml-parsing",
    "ide",
    "language-server-protocol",
    "outline-view"
  ],
  "author": "Te-Chi Liu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liuderchi/ide-yaml/issues"
  },
  "homepage": "https://github.com/liuderchi/ide-yaml#readme",
  "devDependencies": {
    "eslint": "^4.7.1"
  },
  "dependencies": {
    "@types/node": "^6.0.52",
    "atom-languageclient": "^0.6.4",
    "napa": "^3.0.0",
    "typescript": "2.1.5",
    "vscode-languageserver-types": "3.4.0"
  },
  "napa": {
    "yaml-language-server": "redhat-developer/yaml-language-server#0.0.1"
  },
  "engines": {
    "atom": ">=1.21.0"
  },
  "enhancedScopes": [
    "source.yaml"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
